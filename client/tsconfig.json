{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": false,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "checkJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "downlevelIteration": true,
    "sourceMap": true,
    "lib": ["es2020"]
  }
}
